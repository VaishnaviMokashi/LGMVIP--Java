    private void jButton5ActionPerformed(java.awt.event.ActionEvent evt) {                                         

        // TODO add your handling code here:
        jButton5.setEnabled(false);

        if (current_player == 0) {
            obj.array[1][1] = 'X';
            jButton5.setText("X");
            current_player = 1;
            jLabel2.setText("Player 2");
        } else {
            obj.array[1][1] = 'O';
            jButton5.setText("O");
            current_player = 0;
            jLabel2.setText("Player 1");
        }

        obj.print2D();
        announce_winner();
    }                                        

